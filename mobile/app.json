{
  "expo": {
    "name": "Card Counter AI",
    "slug": "card-counter-ai",
    "version": "1.0.5",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1a1a2e"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cardcounterai.app",
      "buildNumber": "13",
      "infoPlist": {
        "NSCameraUsageDescription": "Camera permission is required for future AR Card Counting drills.",
        "NSMicrophoneUsageDescription": "Microphone permission is required for distraction engine features.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1a1a2e"
      },
      "package": "com.cardcounterai.app",
      "permissions": [
        "VIBRATE",
        "CAMERA",
        "RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "card-counter-ai",
    "extra": {
      "eas": {
        "projectId": "444db622-274d-4260-b197-fe1e2f0a3310"
      }
    }
  }
}